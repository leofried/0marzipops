package main.readers;

public class ProductInfoReader {


	public enum Info {

		RAW_NAME(0),
		DESCRIPTION(2),
		AVAILABLE(3),
		DEFAULT_CATEGORY(7),
		CATEGORIES(13),
		SEARCH_TERMS(14),
		ETSY(16),
		EXTRA_PHOTOS(17),
		SOLD_OUT(18);		

		
		private int column;
		private Info(final int column){
			this.column = column;
		}
		public int getColumn(){
			return column;
		}
	}


}
