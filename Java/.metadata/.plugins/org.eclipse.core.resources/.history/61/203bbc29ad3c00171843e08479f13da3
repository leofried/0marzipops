package main.pages;

import main.interfaces.Page;

public class Special extends Page {

	
	
	public Special(String rawName){
		super(rawName, "", Type.SPECIAL);
	}
	
	public String buildTopLine(){
		String write = "";
		write += "<div class='row'><div class='small-12 columns'><p class='fullUnderline zeroMargin popBlue'>";
		write += name + "</p></div></div><br>";
		return write;
	}
	
	
	
	
	
	
}
